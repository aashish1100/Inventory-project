<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Inventory Project</title>
  <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>

  <!-- Navbar -->
 <!-- Navbar -->
<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
  <!-- Left side with Logo -->
  <a class="navbar-brand" href="#">
    <img src="https://yt3.ggpht.com/-DwUvNKX8sYI/AAAAAAAAAAI/AAAAAAAAAAA/J-wQLDjad6A/s900-c-k-no-mo-rj-c0xffffff/photo.jpg" alt="Logo" width="30" height="30" class="d-inline-block align-top">
     GEHU 
  </a>
  
  <!-- Toggler for small screens -->
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  
  <!-- Navbar Links -->
  <div class="collapse navbar-collapse" id="navbarNav">
    <!-- Center-left options -->
    <ul class="navbar-nav mr-auto">
      <li class="nav-item active">
        <a class="nav-link" href="#">Dashboard</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="#">Inventory</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="#">Categories</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="#">Orders</a>
      </li>
    </ul>

    <!-- Right side with Login and Signup -->
    <ul class="navbar-nav ml-auto">
      <li class="nav-item">
        <a class="nav-link" href="#">Login</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="#">Signup</a>
      </li>
    </ul>
  </div>
</nav>


  <!-- Main Content -->
  <div class="container mt-4">
    <div class="row">
      <!-- Dashboard Cards -->
      <div class="col-md-3">
        <div class="card bg-info text-white mb-4">
          <div class="card-body">Total Items</div>
        </div>
      </div>
      <div class="col-md-3">
        <div class="card bg-warning text-white mb-4">
          <div class="card-body">Out of Stock</div>
        </div>
      </div>
      <div class="col-md-3">
        <div class="card bg-success text-white mb-4">
          <div class="card-body">New Orders</div>
        </div>
      </div>
      <div class="col-md-3">
        <div class="card bg-danger text-white mb-4">
          <div class="card-body">Pending Orders</div>
        </div>
      </div>
    </div>

    <!-- Inventory Table -->
    <div class="card mb-4">
      <div class="card-header">
        <h5>Inventory Items</h5>
      </div>
      <div class="card-body">
        <table class="table table-striped">
          <thead>
            <tr>
              <th>SKU</th>
              <th>Name</th>
              <th>Category</th>
              <th>Quantity</th>
              <th>Price</th>
              <th>Actions</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>12345</td>
              <td>Item Name 1</td>
              <td>Category 1</td>
              <td>50</td>
              <td>$10</td>
              <td>
                <button class="btn btn-sm btn-primary">Edit</button>
                <button class="btn btn-sm btn-danger">Delete</button>
              </td>
            </tr>
            <!-- Additional items go here -->
          </tbody>
        </table>
      </div>
    </div>
  </div>

  <!-- Footer -->
  <footer class="bg-dark text-white text-center py-3">
    <div class="container">
      <p class="mb-1">&copy; 2024 InventoryPro. All rights reserved.</p>
      <p>
        <a href="#" class="text-white">About Us</a> |
        <a href="#" class="text-white">Contact Support</a> |
        <a href="#" class="text-white">Privacy Policy</a> |
        <a href="#" class="text-white">Terms of Service</a>
      </p>
    </div>
  </footer>

  <!-- Bootstrap JS and dependencies -->
  <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.4.4/dist/umd/popper.min.js"></script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
